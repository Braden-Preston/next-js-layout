{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "jsx": "preserve",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@atoms": ["./components/defunct/atoms"],
      "@atoms/*": ["./components/defunct/atoms/*"],
      "@stores": ["./models/defunct/stores"],
      "@stores/*": ["./models/defunct/stores/*"],
      "@constants": ["./constants/defunct"],
      "@constants/*": ["./constants/defunct/*"],
      "@functions": ["./functions/defunct"],
      "@functions/*": ["./functions/defunct/*"],
      "@hooks": ["./hooks/defunct"],
      "@hooks/*": ["./hooks/defunct/*"],
      "@utils": ["./utils/defunct"],
      "@utils/*": ["./utils/defunct/*"],
      "@routes": ["./routes"],
      "@routes/*": ["./routes/*"],
      "@models": ["./models/defunct"],
      "@models/*": ["./models/defunct/*"],
      "@components": ["./components/defunct"],
      "@components/*": ["./components/defunct/*"],
      "@templates": ["./components/defunct/templates"],
      "@templates/*": ["./components/defunct/templates/*"],
      "@organisms": ["./components/defunct/organisms"],
      "@organisms/*": ["./components/defunct/organisms/*"],
      "@tpot-scribe-editor": ["./custom/tpot-scribe-editor"],
      "@tpot-scribe-editor/*": ["./custom/tpot-scribe-editor/*"],
      "@ckeditor5-build-fully-featured": ["./custom/ckeditor5-build-fully-featured"],
      "@ckeditor5-build-fully-featured/*": ["./custom/ckeditor5-build-fully-featured/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
